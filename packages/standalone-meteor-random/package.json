{
  "name": "standalone-meteor-random",
  "version": "0.1.0",
  "description": "A drop-in replacement for the 'random' Meteor package",
  "browser": "./dist/browser.js",
  "main": "./dist/server.js",
  "scripts": {
    "test:watch": "jest --watch",
    "watch": "watch 'npm run build' src",
    "test": "jest",
    "clean": "rimraf ./dist",
    "build": "cross-env BABEL_ENV=production babel src --out-dir ./dist",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "watch": "^1.0.2"
  }
}

{
  "name": "standalone-tracker",
  "version": "1.0.1",
  "description": "NPM version of Meteor [Tracker](https://github.com/meteor/meteor/tree/devel/packages/tracker)",
  "main": "lib/tracker.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit __tests__/*",
    "test:watch": "jest --runInBand --detectOpenHandles --forceExit --watch __tests__/*",
    "clean": "rimraf lib",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run test && npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "fibers": "^5.0.0",
    "setimmediate": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harryadel/tracker.git"
  },
  "author": "Harry Adel <harryadelb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harryadel/tracker/issues"
  },
  "homepage": "https://github.com/harryadel/tracker#readme",
  "gitHead": "d4707c98c046f9f4171eed2d76439e55d40c2691"
}

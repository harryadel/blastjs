    "@babel/preset-env": "^7.16.7",
    "cross-env": "^7.0.3",
    "jest": "^27.4.6",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harryadel/blastjs/tree/main/packages/tracker"
  },

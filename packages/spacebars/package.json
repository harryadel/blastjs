  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rimraf": "^2.7.1",
    "watch": "^0.13.0"
  }
}
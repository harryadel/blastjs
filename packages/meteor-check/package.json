{
  "name": "meteor-check",
  "description": "Check whether a value matches a pattern",
  "version": "1.3.1",
  "main": "dist/match.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit __tests__/*",
    "test:watch": "jest --watch",
    "watch": "watch 'npm run build' src",
    "clean": "rimraf ./dist",
    "build": "cross-env BABEL_ENV=production babel src --out-dir ./dist",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "ejson": "2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rimraf": "^2.7.1",
    "watch": "^0.13.0"
  }
}

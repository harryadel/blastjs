    "@blastjs/templating-tools": "^1.2.1",
    "lodash.isempty": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harryadel/blastjs/tree/main/packages/caching-html-compiler"

{
  "name": "blaze",
  "summary": "Meteor Reactive Templating library",
  "version": "2.4.0",
  "git": "https://github.com/meteor/blaze.git",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "watch 'npm run build' src",
    "clean": "rimraf ./dist",
    "build": "cross-env BABEL_ENV=production babel src --out-dir ./dist",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "standalone-tracker": "^1.0.1",
    "observe-sequence": "0.1.0",
    "standalone-reactive-var": "1.0.1",
    "standalone-ordered-dict": "0.1.0",
    "htmljs": "0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  }
}

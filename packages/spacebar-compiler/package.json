{
  "name": "spacebars-compiler",
  "description": "Compiler for Spacebars template language",
  "version": "1.2.1",
  "main": "dist/preamble.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit __tests__/*",
    "clean": "rimraf lib",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run test && npm run build"
  },
  "dependencies": {
    "uglify-js": "2.7.5",
    "htmljs": "0.1.0",
    "html-tools": "0.1.1",
    "blaze-tools": "0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3"
  }
}
{
  "name": "standalone-tracker",
  "version": "1.0.0",
  "description": "NPM version of Meteor [Tracker](https://github.com/meteor/meteor/tree/devel/packages/tracker)",
  "main": "dist/tracker.js",
  "scripts": {
    "lint": "eslint src __tests__",
    "test": "jest --runInBand --detectOpenHandles --forceExit __tests__/*",
    "test:watch": "jest --runInBand --detectOpenHandles --forceExit --watch __tests__/*",
    "clean": "rimraf dist",
    "build": "babel src --out-dir ./dist",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "fibers": "^5.0.0",
    "rimraf": "^3.0.2",
    "setimmediate": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harryadel/tracker.git"
  },
  "author": "Harry Adel <harryadelb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harryadel/tracker/issues"
  },
  "homepage": "https://github.com/harryadel/tracker#readme"
}